# GigNova Environment Variables Example
# Copy this file to .env and fill in your values

# API Configuration
JWT_SECRET=your_jwt_secret_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Development Mode (enables local implementations)
DEV_MODE=true
ENVIRONMENT=dev

# Vector Database (not required in DEV_MODE)
# QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=

# File Storage (not required in DEV_MODE)
# IPFS_API_URL=/ip4/127.0.0.1/tcp/5001

# Blockchain (not required in DEV_MODE)
# WEB3_PROVIDER_URI=http://localhost:8545
# WALLET_PRIVATE_KEY=your_private_key_here

# API Settings
API_TITLE=GigNova API
API_DESCRIPTION=Decentralized freelancing platform with AI agents
API_VERSION=0.1.0

# Agent Configuration
MATCHING_THRESHOLD=0.75
NEGOTIATION_MAX_ROUNDS=3
QA_THRESHOLD=0.8
