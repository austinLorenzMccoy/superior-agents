QDRANT_API_KEY="your_qdrant_api_key"  # Only needed for Qdrant Cloud

# IPFS Configuration
IPFS_API_URL="/ip4/127.0.0.1/tcp/5001"
IPFS_GATEWAY_URL="http://127.0.0.1:8081/ipfs/"

# Blockchain (Sepolia Testnet)
WEB3_PROVIDER_URI="your_web3_provider_uri"  # e.g., https://sepolia.infura.io/v3/your_project_id
